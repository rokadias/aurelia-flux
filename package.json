{
  "name": "aurelia-flux",
  "version": "0.1.7",
  "description": "A Flux dispatcher plugin for Aurelia.",
  "keywords": [
    "aurelia",
    "flux",
    "plugin"
  ],
  "homepage": "https://github.com/tfrydrychewicz/aurelia-flux",
  "bugs": {
    "url": "https://github.com/tfrydrychewicz/aurelia-flux/issues"
  },
  "license": "MIT",
  "author": "Tomasz Frydrychewicz <tomasz.frydrychewicz@gmail.com>",
  "main": "dist/commonjs/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tfrydrychewicz/aurelia-flux"
  },
  "scripts": {
    "test": "./node_modules/.bin/karma start --single-run --browsers Firefox"
  },
  "jspm": {
    "main": "index",
    "format": "amd",
    "directories": {
      "lib": "dist/amd"
    },
    "dependencies": {
      "aurelia-binding": "github:aurelia/binding",
      "aurelia-dependency-injection": "github:aurelia/dependency-injection",
      "aurelia-router": "github:aurelia/router",
      "aurelia-templating": "github:aurelia/templating",
      "bluebird": "npm:bluebird",
      "core-js": "npm:core-js"
    },
    "devDependencies": {
      "babel-runtime": "npm:babel-runtime",
      "core-js": "npm:core-js"
    },
    "peerDependencies": {
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha"
    },
    "overrides": {
      "npm:babel-runtime@5.8.24": {
        "main": false,
        "dependencies": {},
        "optionalDependencies": {
          "core-js": "^1.2.0"
        }
      }
    }
  },
  "devDependencies": {
    "aurelia-tools": "^0.1.0",
    "conventional-changelog": "1.1.0",
    "del": "^2.2.0",
    "es5-shim": "^4.1.5",
    "es6-shim": "^0.35.0",
    "eslint": "^2.8.0",
    "eslint-plugin-flowtype": "^2.2.6",
    "gulp": "^3.8.10",
    "gulp-babel": "^6.1.2",
    "gulp-bump": "^2.1.0",
    "gulp-jshint": "^2.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-yuidoc": "^0.1.2",
    "jasmine-core": "^2.1.3",
    "jshint-stylish": "^2.1.0",
    "jspm": "beta",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^0.2.3",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.2",
    "karma-jspm": "^2.0.1-beta.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-phantomjs-shim": "^1.0.0",
    "object.assign": "^4.0.3",
    "phantomjs": "^2.1.7",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.0",
    "systemjs": "^0.19.26",
    "vinyl-paths": "^2.1.0",
    "yargs": "^4.6.0"
  },
  "aurelia": {
    "usedBy": [],
    "documentation": {
      "links": [
        {
          "rel": "license",
          "mediaType": "text/plain",
          "title": "The MIT License (MIT)",
          "href": "LICENSE"
        },
        {
          "rel": "describedby",
          "mediaType": "application/aurelia-doc+json",
          "title": "API",
          "href": "doc/api.json"
        },
        {
          "rel": "version-history",
          "mediaType": "text/markdown",
          "title": "Change Log",
          "href": "doc/CHANGELOG.md"
        }
      ]
    }
  },
  "dependencies": {
    "babel-eslint": "^6.0.3",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-amd": "^6.6.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.4",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.6.5",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-1": "^6.5.0",
    "jshint": "^2.9.1",
    "phantomjs-prebuilt": "^2.1.7"
  }
}
